<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="career">
	<insert id="insertCareerData" parameterType="CareerDTO">
		
		INSERT INTO CAREER 
		
		VALUES(
		
			#{companyName},
			#{department},
			#{period},
			#{skill},
			#{joinDate},
			#{resignationDate},
			#{humanId}
		
		
		)
	</insert>
	


</mapper>